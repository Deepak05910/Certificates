def init_db():
    conn = sqlite3.connect('students.db')
    c = conn.cursor()
    c.execute('''CREATE TABLE IF NOT EXISTS students (id INTEGER PRIMARY KEY, name TEXT)''')
    c.execute("INSERT INTO students (name) VALUES ('Deepak Saini')")
    c.execute("INSERT INTO students (name) VALUES ('Amit Kumar')")
    c.execute("INSERT INTO students (name) VALUES ('Pooja Sharma')")
    conn.commit()
    conn.close()